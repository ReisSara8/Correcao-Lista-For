import javax.swing.JOptionPane;

public class ExemploTabuada{

   public static void main(String []arg({
   String texto ="";
   
   for(int j =0; i <10; i++){
       for(int i =0; i<10; i++){
           texto += i +"x" + j + "=" + i*j +"      ";
           }
           
           
           
           texto +="\n";
       }
       JOptionPane.showMessageDialog(null, texto);
   
}
   
   
}
